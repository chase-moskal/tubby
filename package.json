{
	"name": "tubby",
	"version": "0.1.13",
	"description": "youtube channel instant search",
	"keywords": [
		"youtube",
		"api-helper",
		"search-bar",
		"video-grid",
		"instant-search"
	],
	"main": "dist/tubby",
	"types": "dist/tubby.d.ts",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"license": "ISC",
	"scripts": {
		"prepare": "npm run build -s && npm test -s",
		"clean": "rimraf dist && mkdirp dist",
		"build": "npm run clean -s && run-p build-scripts build-styles && echo ✔ done production build",
		"build-debug": "npm run clean -s && run-p build-scripts-debug build-styles && echo ✔ done debug build",
		"build-scripts": "tsc && browserify source/tubby.global.tsx -p [ tsify ] -g [ envify --NODE_ENV production ] -g uglifyify > dist/tubby.global.bundle.unoptimized.js && uglifyjs -cm < dist/tubby.global.bundle.unoptimized.js > dist/tubby.global.bundle.js",
		"build-scripts-debug": "tsc && browserify source/tubby.global.tsx --debug -p [ tsify ] > dist/tubby.global.bundle.js",
		"build-styles": "node-sass --source-map true source/tubby.scss dist/tubby.css",
		"watch-styles": "npm run build-styles -s -- --watch",
		"start": "http-server",
		"test": "exit 0"
	},
	"peerDependencies": {
		"mobx": "4.*",
		"mobx-preact": "2.*",
		"preact": "8.*"
	},
	"devDependencies": {
		"browserify": "16.1.1",
		"envify": "4.1.0",
		"es6-promise": "4.2.4",
		"http-server": "0.11.1",
		"mkdirp": "0.5.1",
		"mobx": "4.1.1",
		"mobx-preact": "2.0.0",
		"node-sass": "4.7.2",
		"npm-run-all": "4.1.2",
		"preact": "8.2.7",
		"rimraf": "2.6.2",
		"tsify": "3.0.4",
		"typescript": "2.7.2",
		"uglify-es": "3.3.9",
		"uglifyify": "4.0.5",
		"whatwg-fetch": "2.0.4"
	}
}
